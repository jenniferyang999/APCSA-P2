import java.util.Arrays;
import java.util.Scanner;
import java.util.Collections;
import static java.lang.System.*;

public class FancyWords
{
	private String[] wordRay;
	private String word;

	public FancyWords(String sentence)
	{
		word = sentence;
	}

	public void setWords(String sentence)
	{
		word = sentence;
		wordRay = new String[sentence.length()];
		for (int i = 0; i < 1; i++)
		{
			int blah = sentence.indexOf(" ");
			wordRay[i] = sentence.substring(0, blah);
			//out.println(wordRay[i]);
			//out.println("dafs");
		}
		for (int i = 1; i < wordRay.length; i++)
		{
			int blah = sentence.indexOf(" ");
			int thing = sentence.indexOf(" ");
			wordRay[i] = sentence.substring(blah, thing);
			//out.println(wordRay[i]);
			//out.println("dafs");
		}
	}

	public String toString()
	{
		String output="";
		int max = Integer.MIN_VALUE;
		for (int i = wordRay.length - 1; i >= 0; i--)
		{
			output = output + wordRay[i] + " ";
		}





		return output+"\n\n";
	}
}