import java.util.Arrays;
import java.util.Scanner;
import static java.lang.System.*;
import static java.util.Arrays.*;
import java.io.File;

public class GradeBookFileRunner
{
	static Class test;
	static Student[] student;
   public static void main( String args[] ) throws Exception
   {
		out.println("Welcome to the Class Stats program!");
		
		Scanner file = new Scanner(new File("C:\\Users\\JY\\Desktop\\APCSA-P2\\Unit11\\Unit11-Assignments\\Lab19b\\gradebook.dat"));
		if (file.hasNext())
		{
			out.print(file.next());
			Class test = new Class(file.next(),file.nextInt());

		}
		/*if (file.hasNext())
		{
			file.next();
		}
		*/
		int count = 0;
		while (file.hasNext())
		{
			test.addStudent(count, new Student(file.nextLine(), file.nextLine()));
			count++;
		}
		



	}		
}