import static java.lang.System.*;

public class Lab11c
{
   public static void main( String args[] )
   {
	   int stop=6;
	   for(int r=1; r<=stop; r++) //rows
	   {
	   for(int c=1; c<=r; c++) { //columns 
	   System.out.println("*");
	   }
	   System.out.println();
	   }





	}
}