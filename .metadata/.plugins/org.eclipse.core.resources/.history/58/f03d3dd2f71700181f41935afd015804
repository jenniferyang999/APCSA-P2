public class Lab10a
{
	public static void main( String args[] )
	{
		/*PasswordCheck test = new PasswordCheck();
		test.check();*/
		
		int m=2, total=0;
		while( m < 6 )
		{
		   total = total + m;
		   m = m + 1;
		}
		System.out.print(total);
	}
}