import static java.lang.System.*;
import java.util.Random;


public class Triples
{
   private int number=0, p1, p2, p3;

	public Triples()
	{
		//this(0); //calls another method in here 	
		this.orderNums();
	}

	public Triples(int num)
	{
		number = num;
	}

	public void setNum(int num)
	{
		number = num;
		
	} 
	public void orderNums()
	{
		Random test = new Random();
		int r1 = test.nextInt(number) + 1;
		int r2 = test.nextInt(number) + 1;
		int r3 = test.nextInt(number) + 1;
		if (r1 > r2 && r1 > r3) {
			p3 = r1;
			if (r3 > r2) {
				p2 = r3;
				p1 = r2;
			}
			else {
				p3 = r2;
				p2 = r3;
			}
		}
		else if (r2 > r3 && r2 > r1) {
			p3 = r2;
			if (r3 > r1) {
				p2 = r3;
				p1 = r1;
			}
			else {
				p2 = r1;
				p1 = r3;
			}
		}
		else if (r3 > r2 && r3 > r1) {
			p3 = r3;
			if (r2 > r1) {
				p2 = r2;
				p1 = r1;
			}
			else {
				p2 = r1;
				p1 = r2;
			}
		}
		out.println("1" + r1);
		out.println("2" + r2);
		out.println("3" + r3);
		out.println("1" + p1);
		out.println("2" + p2);
		out.println("3" + p3);  

	}
	
	private int greatestCommonFactor(int a, int b, int c)
	{
		p1 = a;
		p2 = b;
		p3 = c;
		//int max = 0;

	   while ((a != 0 && b != 0) && (c != 0))
       {
           if (a >= b && a >=c)
           {
               int a1 = a - b;
           }
          // else if ()
               b = b - a;
       }
       if (a == 0) {
	     return 1;
       }
       else {
     	  return 1;
       }
	}

	public String toString()
	{
		String output="";






		return output+"\n";
	}
}