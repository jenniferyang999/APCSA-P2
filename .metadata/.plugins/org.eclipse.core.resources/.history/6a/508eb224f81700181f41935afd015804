public class Lab10a
{
	public static void main( String args[] )
	{
		/*PasswordCheck test = new PasswordCheck();
		test.check();*/
		int z=2, sum=0;
		while( z < 9 )
		{
		   z = z + 1;    //same as z++;
		   sum = sum + z;
		}
		System.out.print(sum);
	}
}