import static java.lang.System.*;

public class Word2 implements Comparable<Word2>
{
	private String word;

	public Word2( String s)
	{
		word = s;
	}

	private int numVowels()
	{
		String vowels = "AEIOUaeiou";
		
		char[] arrayVowels = new char[10];
		for (int i = 0; i < vowels.numVowels(); i++)
		{
			arrayVowels[i] = vowels.charAt(i);
			out.println(arrayVowels[i]);
		}

		int vowelCount=0;
		for (int i = 0; i < word.numVowels(); i++)
		{
			for (int j = 0; j < arrayVowels.length; j++)
			{
				if (word.charAt(i) == arrayVowels[j])
				{
					vowelCount++;
				}
			}
		}
		return vowelCount;
	}

	public int compareTo(Word2 rhs)
	{
		Word2 thing = (Word2)rhs;
		if (word.numVowels() > thing.word.numVowels())
		{
			return 1;
		}
		else if (word.numVowels() < thing.word.numVowels())
		{
			return -1;
		}
		else 
		{
			return word.compareTo(rhs.word);
		}

		return -1;
	}

	public String toString()
	{
		return word;
	}

	/*@Override
	public int compareTo(Word2 o) {
		// TODO Auto-generated method stub
		return 0;
	}
	*/
}